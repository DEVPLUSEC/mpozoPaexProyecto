<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mpozoPaexProyecto.paginas.inicio"
             Title="Inicio"
             IconImageSource="inicio">
    <ContentPage.Content>
        <StackLayout>

            <Frame CornerRadius="15"  Padding="15" HasShadow="False" HorizontalOptions="CenterAndExpand">
                <StackLayout>
                    <Label FontSize="25" Text="Informacion General" TextColor="Black"/>
                </StackLayout>
            </Frame>

            <Frame Margin="25">
                <Label x:Name="txtNombres"></Label>
            </Frame>
            <Frame Margin="25">
                <Label x:Name="txtCorreo"></Label>
            </Frame>
            
            



        </StackLayout>
    </ContentPage.Content>
</ContentPage>